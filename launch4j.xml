<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>liv/lanslim.jar</jar>
  <outfile>liv/lanslim.exe</outfile>
  <errTitle>Unable to download Java Virtual Machine</errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <icon>comments.ico</icon>
  <singleInstance>
    <mutexName>LANSLIM</mutexName>
    <windowTitle>LANSLIM</windowTitle>
  </singleInstance> 
  <jre>
    <path>%JAVA_HOME%</path>
    <minVersion>1.4.2</minVersion>
    <maxVersion></maxVersion>
    <dontUsePrivateJres>false</dontUsePrivateJres>
    <initialHeapSize>0</initialHeapSize>
    <maxHeapSize>0</maxHeapSize>
  </jre>
  <versionInfo>
    <fileVersion>0.6.0.0</fileVersion>
    <txtFileVersion>V0.6</txtFileVersion>
    <fileDescription>Deployment version</fileDescription>
    <copyright>Created by Olivier Mourez but free to use</copyright>
    <productVersion>0.6.0.0</productVersion>
    <txtProductVersion>V0.6</txtProductVersion>
    <productName>LANSLIM (Local Area Network Instant Messaging)</productName>
    <companyName>None</companyName>
    <internalName>lanslim</internalName>
    <originalFilename>lanslim.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
